ction;\n};\n\nexport const convertCollectionsSnapshotToMap = (collections: QuerySnapshot) => {\n  const transformedCollections = collections.docs.map((doc) => {\n    const { title, items } = doc.data();\n\n    return {\n      routeName: encodeURI(`${title.toLowerCase()}`),\n      id: doc.id,\n      title,\n      items,\n    };\n  });\n\n  return transformedCollections.reduce((acc, collection) => {\n    // @ts-ignore\n    acc[collection.title.toLowerCase()] = collection;\n    return acc;\n  }, {});\n};\n\nexport const getCurrentUser = () => {\n  return new Promise((resolve, reject) => {\n    const unsubscribe = onAuthStateChanged(\n      auth,\n      (userAuth) => {\n        unsubscribe();\n        resolve(userAuth);\n      },\n      reject\n    );\n  });\n};\n\nexport default { db };\n","/home/arnaldo/web-projects/portfolio/life-line-app/client/src/redux/cart/cart-actions.tsx",[],"/home/arnaldo/web-projects/portfolio/life-line-app/client/src/redux/shop/shop-types.tsx",[],"/home/arnaldo/web-projects/portfolio/life-line-app/client/src/redux/cart/cart.utils.tsx",[],"/home/arnaldo/web-projects/portfolio/life-line-app/client/src/redux/cart/cart-types.tsx",[],"/home/arnaldo/web-projects/portfolio/life-line-app/client/src/redux/shop/shop-data.tsx",[],"/home/arnaldo/web-projects/portfolio/life-line-app/client/src/redux/directory/directory-selectors.tsx",[],"/home/arnaldo/web-projects/portfolio/life-line-app/client/src/components/custom-button/CustomButton.tsx",[],"/home/arnaldo/web-projects/portfolio/life-line-app/client/src/components/menu-item/MenuItem.tsx",[],"/home/arnaldo/web-projects/portfolio/life-line-app/client/src/components/form-input/FormInput.tsx",[],"/home/arnaldo/web-projects/portfolio/life-line-app/client/src/components/cart-item/CartItem.tsx",[],{"ruleId":"203","severity":1,"message":"204","line":2,"column":15,"nodeType":"205","messageId":"206","endLine":2,"endColumn":25},{"ruleId":"203","severity":1,"message":"207","line":98,"column":6,"nodeType":"205","me